@model ATSM.Ingenieria.ComponenteMayor
@{ 
	ViewBag.Title = Model.Codigo;
}
<div class="container">
    <div class="shadow-sm p-2 mb-2 bg-light rounded"><h3 id="tit">@Model.Descripcion</h3></div>

    <ul class="nav nav-tabs" id="ComponenteMayor" role="tablist">
        <li class="nav-item" role="presentation">
            <button class="nav-link active" id="Estructura-tab" data-bs-toggle="tab" data-bs-target="#Estructura" type="button" role="tab" aria-controls="Estructura" aria-selected="true">Estructura</button>
        </li>
        <li class="nav-item" role="presentation">
            <button class="nav-link" id="Alta-tab" data-bs-toggle="tab" data-bs-target="#Alta" type="button" role="tab" aria-controls="Alta" aria-selected="false">Alta</button>
        </li>
    </ul>
    <div class="tab-content" id="myTabContent">
        <div class="tab-pane fade show active" id="Estructura" role="tabpanel" aria-labelledby="Estructura-tab">
            @RenderPage("~/Areas/Ingenieria/Views/ComponenteMayor/Mayores/Arbol.cshtml")
        </div>
        <div class="tab-pane fade" id="Alta" role="tabpanel" aria-labelledby="Alta-tab">
            @RenderPage("~/Areas/Ingenieria/Views/ComponenteMayor/Mayores/Alta.cshtml")
        </div>
    </div>
</div>
@section Scripts{
    @Scripts.Render("~/Areas/Ingenieria/cjs/ItemMayor.js")
    @Scripts.Render("~/Areas/Ingenieria/cjs/Modelo.js")
    @Scripts.Render("~/Areas/Ingenieria/cjs/Tiempos.js")
    @Scripts.Render("~/Areas/Ingenieria/Views/ComponenteMayor/Mayores/appr.js")
}