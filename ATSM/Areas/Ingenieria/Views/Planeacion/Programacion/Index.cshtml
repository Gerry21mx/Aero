@{ 
	ViewBag.Title = "Programacion";
}
<div class="container">
	<div class="row mt-3">
		<div class="col-12 col-md-3">
            @Campos.SelectAircraft(true,"IdAircraft","","Aeronave",true)
        </div>
		<div class="col-12 col-md-3">
            @Campos.SelectComponentesMayores(true)
        </div>
		<div class="col-12 col-md-3">
            @Campos.SelectFamily(true)
        </div>
		<div class="col-12 col-md-3">
			<button id="con" class="btn btn-sm btn-outline-success">Consulta</button>
        </div>
    </div>
	<div class="row table-responsive mt-3">
		<table id="tabla" class="table table-sm table-hovered table-striped table-bordered sz-font-725">
			<thead class="table-primary">
				<tr>
					<th colspan="6"></th>
					<th colspan="4">Remanentes</th>
					<th colspan="3">Limites</th>
				</tr>
				<tr>
					<th>Id</th>
					<th>Familia</th>
					<th>Parte</th>
					<th>Descripcion</th>
					<th>Serie</th>
					<th>PIC</th>
					<th>Horas</th>
					<th>Ciclos</th>
					<th>Dias</th>
					<th>Fecha</th>
					<th>Horas</th>
					<th>Ciclos</th>
					<th>Dias</th>
				</tr>
            </thead>
        </table>
    </div>
</div>
@section Scripts{
    @Scripts.Render("~/Areas/Ingenieria/cjs/ItemMenor.js")
}
