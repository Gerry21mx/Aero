@{
	ViewBag.Title = "Modelos";
}

<div class="container mt-0 mt-md-2">
    <div class="shadow-sm p-2 mb-2 bg-light rounded"><h3>@ViewBag.Title</h3></div>

    <div class="row justify-content-around">
	    <div class="col-12 col-md-5">
            @RenderPage("~/Areas/Ingenieria/Views/Catalogos/Modelos/Alta.cshtml")
        </div>
	    <div class="col-12 col-md-7 sz-font-75">
            @RenderPage("~/Areas/Ingenieria/Views/Catalogos/Modelos/List.cshtml")
        </div>
    </div>
</div>

@section Scripts{
    @Scripts.Render("~/Areas/Ingenieria/cjs/Modelo.js")
    @Scripts.Render("~/Areas/Ingenieria/cjs/ComponenteMayor.js")
    @Scripts.Render("~/Areas/Ingenieria/Views/Catalogos/Modelos/appr.js")
}