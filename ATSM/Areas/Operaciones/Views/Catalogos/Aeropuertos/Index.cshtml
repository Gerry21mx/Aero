@{
    ViewBag.Title = "Aeropuertos";
}

<div class="container mt-0 mt-md-2">
    <div class="shadow-sm p-2 mb-2 bg-light rounded"><h3>Aeropuertos</h3></div>
    <nav>
        <div class="nav nav-tabs" id="nav-tab" role="tablist">
            <a class="nav-item nav-link active" id="nav-aeropuertos-tab" data-bs-toggle="tab" href="#nav-aeropuertos" role="tab" aria-controls="nav-aeropuertos" aria-selected="true">Aeropuertos</a>
            <a class="nav-item nav-link" id="nav-alta-tab" data-bs-toggle="tab" href="#nav-alta" role="tab" aria-controls="nav-alta" aria-selected="false">Alta</a>
            <a class="nav-item nav-link" id="nav-gastos-tab" data-bs-toggle="tab" href="#nav-gastos" role="tab" aria-controls="nav-gastos" aria-selected="false">Gastos</a>
        </div>
    </nav>
    <div class="tab-content" id="nav-tabContent">
        <div class="tab-pane fade show active" id="nav-aeropuertos" role="tabpanel" aria-labelledby="nav-aeropuertos-tab">
            @RenderPage("~/Areas/Operaciones/Views/Catalogos/Aeropuertos/List.cshtml")
        </div>
        <div class="tab-pane fade" id="nav-alta" role="tabpanel" aria-labelledby="nav-alta-tab">
            @RenderPage("~/Areas/Operaciones/Views/Catalogos/Aeropuertos/Alta.cshtml")
        </div>
        <div class="tab-pane fade" id="nav-gastos" role="tabpanel" aria-labelledby="nav-gastos-tab">
            @RenderPage("~/Areas/Operaciones/Views/Catalogos/Aeropuertos/Gastos.cshtml")
        </div>
    </div>
</div>


@section Scripts{
    
    @Scripts.Render("~/Areas/Seguimiento/cjs/Aeropuerto.js")
    @Scripts.Render("~/Areas/Operaciones/Views/Catalogos/Aeropuertos/appr.js")
}