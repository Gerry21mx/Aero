@{ 
	ViewBag.Title = "Devolucion";
}
<div class="container mt-0 mt-md-2">
    <div class="shadow-sm p-2 mb-2 bg-light rounded"><h3>Devolucion a Cuenta</h3></div>
    <nav>
        <div class="nav nav-tabs" id="nav-tab" role="tablist">
            <a class="nav-item nav-link active" id="nav-lista-tab" data-bs-toggle="tab" href="#nav-lista" role="tab" aria-controls="nav-lista" aria-selected="true">Cuentas</a>
            <a class="nav-item nav-link disabled" id="nav-fondo-tab" data-bs-toggle="tab" href="#nav-fondo" role="tab" aria-controls="nav-fondo" aria-selected="false">Fondo</a>
            <a class="nav-item nav-link disabled" id="nav-anticipo-tab" data-bs-toggle="tab" href="#nav-anticipo" role="tab" aria-controls="nav-anticipo" aria-selected="false">Anticipo</a>
        </div>
    </nav>
    <div class="tab-content" id="nav-tabContent">
        <div class="tab-pane fade show active pt-3 sz-font-75" id="nav-lista" role="tabpanel" aria-labelledby="nav-lista-tab">
            @RenderPage("~/Areas/Cuentas/Views/Movimientos/List.cshtml")
        </div>
        <div class="tab-pane fade" id="nav-fondo" role="tabpanel" aria-labelledby="nav-fondo-tab">
            @RenderPage("fondo.cshtml")
        </div>
        <div class="tab-pane fade" id="nav-anticipo" role="tabpanel" aria-labelledby="nav-fondo-tab">
            @RenderPage("anticipo.cshtml")
        </div>
    </div>
	
</div>
<script> const act = 1; </script>
@section Scripts{
    @Scripts.Render("~/Areas/Cuentas/cjs/Account.js")
    @Scripts.Render("~/Areas/Cuentas/cjs/Transaccion.js")
    @Scripts.Render("~/Areas/Cuentas/Views/Movimientos/Devolucion/devolucion.js")
}