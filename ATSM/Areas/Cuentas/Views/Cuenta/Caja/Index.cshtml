@{ 
	ViewBag.Title = "Caja Chica";
	AccountType tipo = new AccountType("Caja Chica");
}
<div class="container mt-0 mt-md-2">
    <div class="shadow-sm p-2 mb-2 bg-light rounded"><h3>Caja Chica</h3></div>
    <nav>
        <div class="nav nav-tabs" id="nav-tab" role="tablist">
            <a class="nav-item nav-link active" id="nav-lista-tab" data-bs-toggle="tab" href="#nav-lista" role="tab" aria-controls="nav-lista" aria-selected="true">Caja Chica</a>
            <a class="nav-item nav-link" id="nav-alta-tab" data-bs-toggle="tab" href="#nav-alta" role="tab" aria-controls="nav-alta" aria-selected="false">Alta</a>
        </div>
    </nav>
    <div class="tab-content" id="nav-tabContent">
        <div class="tab-pane fade show active pt-3 sz-font-75" id="nav-lista" role="tabpanel" aria-labelledby="nav-lista-tab">
            @RenderPage("~/Areas/Cuentas/Views/Cuenta/List.cshtml")
        </div>
        <div class="tab-pane fade" id="nav-alta" role="tabpanel" aria-labelledby="nav-alta-tab">
            @RenderPage("~/Areas/Cuentas/Views/Cuenta/Alta.cshtml")
        </div>
    </div>
	
</div>

<script> const act = @tipo.Id; </script>
@section Scripts{
    @Scripts.Render("~/Areas/Cuentas/cjs/Account.js")
    @Scripts.Render("~/Areas/Cuentas/Views/Cuenta/cuenta.js")
}